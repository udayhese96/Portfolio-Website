{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/send-email.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "VITE_APP_URL": "https://udayhese96-portfolio.vercel.app"
  }
}